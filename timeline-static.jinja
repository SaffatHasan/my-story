<head>
<link href="static.css" rel="stylesheet"/>
</head>
<body>
  <div class="timeline">
    {%- for event in timeline -%}
    {%- if loop.index % 2 == 0 -%}
    <div class="container left">
    {%- else -%}
    <div class="container right">
    {%- endif -%}
      <div class="content">
        <h2>{{ event.date }}</h2>
        <p>{{ event.text }}</p>
      </div>
    </div>
    {%- endfor -%}
    </div>
  </div>
</body>